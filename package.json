{
  "name": "generator-ecliptic",
  "version": "1.0.4",
  "description": "Ecliptic's Yeoman generators for scaffolding new applications",
  "repository": "https://github.com/ecliptic/generator-ecliptic.git",
  "author": "Brandon Konkle <brandon@ecliptic.io>",
  "license": "Apache-2.0",
  "files": [
    "generators"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "scripts": {
    "format": "prettier-eslint --write \"generators/**/*.js\" --ignore \"generators/*/templates/**/*\""
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "figlet": "^1.2.0",
    "mkdirp": "^0.5.1",
    "yeoman-generator": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.7.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.54.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.2.1",
    "prettier-eslint-cli": "^4.3.2"
  },
  "lint-staged": {
    "*.js": [
      "yarn run format",
      "git add"
    ]
  }
}
