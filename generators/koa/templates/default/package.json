{
  "name": "<%= name %>",
  "version": "0.1.0-beta.1",
  "description": "<%= description %>",
  "private": true,
  "main": "src/Main.js",
  "repository": {
    "type": "git",
    "url": "<%= repo %>"
  },
  "author": "<%= author %>",
  "license": "UNLICENSED",
  "keywords": [
<% keywords.forEach(function (keyword, i) { -%>
    "<%= keyword %>"<% if (i !== keywords.length - 1) { %>,<% } %>
<% }) -%>
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test NODE_PATH=src:node_modules jest",
    "test.watch": "yarn test -- --watch",
    "precommit": "lint-staged",
    "format": "prettier-eslint --write \"packages/*/src/**/*.js\"",
    "dev": "NODE_ENV=development dotenv node src/Main.js",
    "dev.watch": "NODE_ENV=development nodemon -x dotenv node src/Main.js",
    "start": "NODE_ENV=production node src/Main.js",
    "install.flow": "echo \"Installing Flow types...\" && flow-typed install -s",
    "lint": "eslint --ext .js,.jsx ./",
    "lint.types": "echo \"Checking Flow types...\" && flow",
    "postinstall": "yarn install.flow",
    "precommit": "lint-staged",
    "prepush": "yarn lint.types && yarn test"
  },
  "dependencies": {
    "ramda": "^0.24.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^5.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.55.0",
    "flow-typed": "^2.1.5",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.0",
    "lint-staged": "^4.2.3",
    "prettier-eslint-cli": "^4.4.0"
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "identity-obj-proxy"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ]
  },
  "lint-staged": {
    "*.js": [
      "format",
      "git add"
    ]
  }
}
