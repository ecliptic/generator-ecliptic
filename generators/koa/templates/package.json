{
  "name": "<%= name %>",
  "version": "0.1.0-beta.1",
  "description": "<%= description %>",
  "private": true,
  "main": "src/Main.js",
  "repository": {
    "type": "git",
    "url": "<%= repo %>"
  },
  "author": "<%= author %>",
  "license": "UNLICENSED",
  "keywords": [
<% keywords.forEach(function (keyword, i) { -%>
    "<%= keyword %>"<% if (i !== keywords.length - 1) { %>,<% } %>
<% }) -%>
  ],
  "engines": {
    "node": "8.4.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel -d build src",
    "watch": "yarn build -- -w",
    "clean": "rm -rf public",
    "test": "cross-env NODE_PATH=src:node_modules jest",
    "test-watch": "yarn test -- --watch",
    "precommit": "lint-staged",
    "format": "prettier-eslint --write \"packages/*/src/**/*.js\""
  },
  "dependencies": {
    "history": "^4.6.1",
    "ramda": "^0.23.0",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "recompose": "^0.23.4",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0",
    "styled-components": "^1.4.6"
  },
  "devDependencies": {
    "@webpack-blocks/babel6": "^0.4.1",
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/extract-text2": "^0.4.0",
    "@webpack-blocks/sass": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.0.11",
    "cross-env": "^5.0.0",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.33.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.46.0",
    "husky": "^0.13.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.3",
    "jest-styled-components": "^2.0.0",
    "lint-staged": "^3.4.2",
    "nightwatch": "^0.9.15",
    "prettier-eslint-cli": "^3.4.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.2.3",
    "webpack": "^2.5.1",
    "webpack-blocks-copy": "^1.0.2",
    "webpack-blocks-html": "^1.3.1",
    "webpack-blocks-utils": "^1.3.1",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "testURL": "http://localhost",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "identity-obj-proxy"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom"
    ]
  },
  "lint-staged": {
    "*.js": [
      "yarn run format",
      "git add"
    ]
  }
}
